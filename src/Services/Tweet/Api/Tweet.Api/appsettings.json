  "ConnectionStrings": {
    "DefaultConnection": "Data Source=:memory:;Version=3;New=True;"
  },
  "ContentChecker": {
    "Url": "https://contentfilter.azurewebsites.net/api/contentfilter",
    "Enabled": true
  },
  "Kestrel": {
